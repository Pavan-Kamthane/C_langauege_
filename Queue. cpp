#include <iostream>
#include <queue>

using namespace std;

int main() {

  // create a queue of string
  queue<string> animals;

  // push elements into the queue
  animals.push("Cat");
  animals.push("Dog");
  
  cout << "Queue: ";

  while(!animals.empty()) {

    cout << animals.front() << ", ";

    animals.pop();
  }

  cout << endl;
 
  return 0;
}
